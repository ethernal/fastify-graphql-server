{
	"name": "variables",
	"version": "1.0.0",
	"main": "./src/server.ts",
	"license": "MIT",
	"scripts": {
		"typescript:verify:install": "npx ts-node src/index.ts",
		"test": "cross-env NODE_ENV=test jest --coverage",
		"test:watch": "cross-env NODE_ENV=test jest --coverage --watchAll",
		"build": "tsc",
		"build:watch": "onchange 'src/**/*.ts' -- npm run build",
		"view:coverage": "serve coverage/lcov-report",
		"clean": "rm -rf coverage dist",
		"dev": "nodemon ./src/server.ts",
		"migrate": "npx prisma migrate dev",
		"generate": "npx prisma generate",
		"start": "node src/index.js"
	},
	"dependencies": {
		"@prisma/client": "2.28.0",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"fastify": "^3.20.1",
		"mercurius": "^8.1.1",
		"mercurius-integration-testing": "^3.2.0"
	},
	"devDependencies": {
		"@graphql-tools/graphql-file-loader": "^7.0.2",
		"@graphql-tools/load": "^7.1.2",
		"@graphql-tools/schema": "^8.0.3",
		"@graphql-typed-document-node/core": "^3.1.0",
		"@types/jest": "^26.0.23",
		"@types/node": "^16.4.13",
		"@types/source-map-support": "^0.5.4",
		"jest": "^27.0.5",
		"mercurius-codegen": "^1.8.0",
		"nodemon": "^2.0.12",
		"onchange": "^7.1.0",
		"prisma": "2.28.0",
		"serve": "^12.0.0",
		"source-map-support": "^0.5.19",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.1.0",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	}
}
